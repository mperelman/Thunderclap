# Environment variables for secure deployment
# Copy this to .env and fill in your actual values
# DO NOT commit .env to Git!

# API Keys (rotate through these)
GEMINI_API_KEY_1=your_key_here
GEMINI_API_KEY_2=your_key_here
GEMINI_API_KEY_3=your_key_here
GEMINI_API_KEY_4=your_key_here
GEMINI_API_KEY_5=your_key_here
GEMINI_API_KEY_6=your_key_here

# Security
SECRET_KEY=generate_random_secret_here
ALLOWED_ORIGINS=https://your-frontend.com

# Rate limiting
MAX_REQUESTS_PER_HOUR=10
MAX_REQUESTS_PER_DAY=50




